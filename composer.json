{
    "name": "knackline/laravel-listmonk",
    "description": "Laravel package for Listmonk API integration",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "listmonk", "newsletter", "api-client"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Knackline\\Listmonk\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Knackline\\Listmonk\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Knackline\\Listmonk\\Providers\\ListmonkServiceProvider"
            ],
            "aliases": {
                "Listmonk": "Knackline\\Listmonk\\Facades\\Listmonk"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Rajkumar Samra"
        }
    ]
}
